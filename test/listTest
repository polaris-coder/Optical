N = 10 #以10阶为例演示
YHTriangle = []
for i in range(N):  # 行
    YHTriangle.append([])
    if i == 0:
        YHTriangle[i].append(1) #第一行只有 1
    else:
        YHTriangle[i].append(1) #最左元素永远为 1
        YHTriangle[i].append(1) #最右元素永远为 1
        for j in range(1,i):    #中间元素
            if i != 0 and i != 1:
                YHTriangle[i].insert(j,YHTriangle[i-1][j-1] + YHTriangle[i-1][j])
for i in range(N):
    print(YHTriangle[i])